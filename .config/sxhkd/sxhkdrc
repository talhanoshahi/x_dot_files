# My Hotkeys

super + space
    j4-dmenu-desktop

Print
    maim -s | xclip -selection clipboard -t image/png

super + shift + Print
    maim -s | tee ~/pix/ss/$(date +%s).jpg | xclip -selection clipboard -t image/jpg

super + control + Print
    maim | tee ~/pix/ss/$(date +%s).jpg | xclip -selection clipboard -t image/jpg

super + shift + Return
    /usr/bin/firefox

super + shift + e
    emoji

super + b
    print_bookmark

super + shift + b
    add_bookmark

XF86Audio{RaiseVolume,LowerVolume,Mute}
    {pactl_increase_volume,pactl_decrease_volume,pactl_toggle_default_sink}

XF86MonBrightness{Up,Down}
    brightnessctl s {+2%,2%-}

XF86TouchpadToggle
    toggle_touchpad

super + Escape
    system_action

super + z
    magnus

super + c
    zs
